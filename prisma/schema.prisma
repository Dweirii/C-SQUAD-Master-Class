generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DiscountCode {
  id         String   @id @default(cuid())
  code       String   @unique
  percentage Int
  ownerName  String
  createdAt  DateTime @default(now())
}

model FreeOrder {
  id        String   @id @default(cuid())
  name      String
  phone     String
  email     String
  code      String
  createdAt DateTime @default(now())
}
